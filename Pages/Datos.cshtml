@page
@model WebApplication1.Pages.DatosModel
@{
}

<form method="post">
    <div class="form-group">
        <label for="Nota.Nombre">Nombre:</label>
        <input type="text" class="form-control" id="Nota.Nombre" asp-for="Nota.Nombre">
    </div>

    <div class="form-group">
        <label for="Nota.Materia">Materia:</label>
        <input type="text" class="form-control" id="Nota.Materia" asp-for="Nota.Materia">
    </div>

    <div class="form-group">
        <label for="Nota.Parcial1">Parcial 1:</label>
        <input type="text" class="form-control" id="Nota.Parcial1" asp-for="Nota.Parcial1">
    </div>

    <div class="form-group">
        <label for="Nota.Parcial2">Parcial 2:</label>
        <input type="text" class="form-control" id="Nota.Parcial2" asp-for="Nota.Parcial2">
    </div>

    <div class="form-group">
        <label for="Nota.ExamenFinal">Examen Final:</label>
        <input type="text" class="form-control" id="Nota.ExamenFinal" asp-for="Nota.ExamenFinal">
    </div>

    <button type="submit" class="btn btn-primary">Enviar</button>
</form>

